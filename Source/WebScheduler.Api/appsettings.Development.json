{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Information"
      }
    }
  },
  "ForwardedHeaders": {
    "AllowedHosts": [
      "*"
    ]
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "https://*:5001"
      }
    }
  },
  "Cors": {
    "Origin": "https://localhost:7099"
  },
  "Identity": {
    "Authority": "https://account.nullreference.io",
    "Resource": "https://account.nullreference.io/resources"
  }
}
